# RAP Generator Environment Variables

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=True
PORT=5001

# OpenAI API (optional - for intelligent parsing fallback)
OPENAI_API_KEY=your_openai_api_key_here

# CORS Settings (your WordPress site URL)
ALLOWED_ORIGINS=http://localhost:3000,https://yourdomain.com

# File Upload Settings
MAX_FILE_SIZE_MB=50
ALLOWED_EXTENSIONS=pdf

# Logging
LOG_LEVEL=INFO